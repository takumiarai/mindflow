{
	"name": "My First Extension",
	"description": "A description for my new app.",
	"version": "0.1",
	"permissions": [
    	"notifications"
    ],
	"manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png",
		"default_title": "Start a timer"
	}
}
